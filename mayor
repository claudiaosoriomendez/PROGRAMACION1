#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;


int main ()
{ 
	int i,n,mayor,x;
	mayor = -999;

	cout << "Ingrese cuantos numeros tiene su serie: ";
	cin >> n;
	
	for (i=1;i<=n;i++)
	{ 
		cout << "Ingrese numero: "<< endl;
		cin >> x;

		if(mayor<x)
		{ 
		mayor = x;
		}

	    
	}
	cout <<"El mayor de su serie es: "<< mayor << endl;
	getch ();
	return 0;

}

